<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.discount.MemberJoinDiscount" table="MEMBERJOINDISCOUNT">
		<id name="id" column="ID" type="long">
			<generator class="identity" />
		</id>
		<property name="memberid" column="MEMBERID" type="long"/>
		<property name="discountid" column="DISCOUNTID" type="long"/>
		<property name="addtime" column="ADDTIME" type="timestamp"/>
		<property name="validtime" column="VALIDTIME" type="timestamp"/>
	</class>
</hibernate-mapping>

