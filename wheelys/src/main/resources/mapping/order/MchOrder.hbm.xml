<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.order.MchOrder" table="MCHORDER">
		<id name="id" column="ID" type="long">
			<generator class="identity" />
		</id>
		
		<property name="tradeno" column="TRADENO" type="string" />
		<property name="ordertitle" column="ORDERTITLE" type="string" />
		<property name="mchid" column="MCHID" type="long" />
		<property name="shopid" column="SHOPID" type="long" />
		<property name="mchname" column="MCHNAME" type="string" />
		<property name="shopname" column="SHOPNAME" type="string" />
		<property name="citycode" column="CITYCODE" type="string" />
		<property name="cityname" column="CITYNAME" type="string" />
		<property name="status" column="STATUS" type="string" />
		<property name="paystatus" column="PAYSTATUS" type="string" />
		<property name="username" column="USERNAME" type="string" />
		<property name="mobile" column="MOBILE" type="string" />
		<property name="totalfee" column="TOTALFEE" type="integer" />
		<property name="quantity" column="QUANTITY" type="integer" />
		<property name="discount" column="DISCOUNT" type="integer" />
		<property name="payfee" column="PAYFEE" type="integer" />
		<property name="netpaid" column="NETPAID" type="integer" />
		<property name="createtime" column="CREATETIME" type="timestamp" />
		<property name="updatetime" column="UPDATETIME" type="timestamp" />
		<property name="paidtime" column="PAIDTIME" type="timestamp" />
		<property name="sendtime" column="SENDTIME" type="timestamp" />
	</class>
</hibernate-mapping>

