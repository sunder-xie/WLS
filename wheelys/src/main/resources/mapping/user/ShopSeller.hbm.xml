<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.ShopSeller" table="wheelys_seller" dynamic-update="true">
		<id name="id" column="sellerid" type="long">
			<generator class="identity" />
		</id>
		<property name="shopid" column="sid" type="long" />
		<property name="loginname" column="usernumber" type="string" />
		<property name="username" column="username" type="string" />
		<property name="userpwd" column="userpwd" type="string" />
		<property name="status" column="status" type="integer" />
		</class>
</hibernate-mapping>