   	 			alert('规划未来的发展信息不能为空')
   	 			isokey  = false;
   	 			return false;
   	 		}
   			
   			if($(a).attr('name')=='values6'&& $(a).val()==''){
   	 			alert('最大的困难和障碍信息不能为空')
   	 			isokey = false;
   	 			return false;
