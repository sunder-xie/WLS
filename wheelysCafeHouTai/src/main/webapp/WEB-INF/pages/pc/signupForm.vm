   	 			isokey  = false;
   	 			return false;
   	 		}
   			if($(a).attr('name')=='values8'&& $(a).val()==''){
   	 			alert('第一条产业基金信息不能为空')
   	 			isokey  = false;
   	 			return false;
   	 		}
   			if($(a).attr('name')=='values9'&& $(a).val()==''){
   	 			alert('第二条产业基金信息不能为空')
   	 			isokey  = false;
   	 			return false;
   	 		}	
   	  })

   	  if(isokey)submitForm_2()
   	}

          function submitForm_2(){
       		var data = {};
       		var valNull = true;
       		$('#inputList input').each(function(e,a){
       			var attr = $(a).attr('name');
       			if($(a).attr('data-value') == 'must'){
       				if($(a).val()==''){
       					valNull = false;
       					return
       				}else{
       					data[attr] = $(a).val();
       				}
       			}else{
           			data[attr] = $(a).val();
       			}
       		});
       		$('#inputList textarea').each(function(e,a){
       			var attr = $(a).attr('name');
       			data[attr] = $(a).val();
       		});
       		if(valNull){
           		$.ajax({
  	       		      url:"${basePath}pc/addSignupForm.xhtml",
  	       		      type:'post',
  	       		      dataType:'json',
  	       		      data:data,
  	       		      success:function(data){
  	       		    	  if(data.success){
  	       		    		location.href = '${basePath}pc/info.xhtml';
  	       		    	  }else{
  	       		    		alert(data.msg);   
  	       		    	  }
  	       		      }
  	       		});
       		}else{
       			alert('必填项不能为空')
       		}
       	}
