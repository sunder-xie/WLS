    <script src="${pcPath}Public/home/js/jquery-1.11.2.min.js"></script>
    <script>
    function submitForm(){
        var isokey = true;
      	$('#inputList input').each(function(e,a){
      		if($(a).attr('name')=="name" && $(a).val()==''){
      			alert("姓名不能为空")
      			isokey = false
      			return false
      		}
      		if($(a).attr('name')=="idcard" && $(a).val()==''){
      			alert("身份证不能为空")
      			isokey = false
      			return false
      		}
      		
      		if($(a).attr('name')=="idcard" && !(/^(\d{15}|\d{17}[0-9xX]{1})$/.test($(a).val()))){
      			alert("身份证有误")
      			isokey = false
      			return false
      		}
      		
      		if($(a).attr('name')=="phone" && $(a).val()==''){
      			alert("手机号不能为空")
      			isokey = false
      			return false
      		}
      		if($(a).attr('name') == 'phone' && !(/^0?1[3|4|5|8|7][0-9]\d{8}$/.test($(a).val()))){
       			alert('手机有误');
       			isokey = false;
       			return false;
       		}
      		if($(a).attr('name')=="email" && $(a).val()==''){
      			alert("邮箱不能为空")
      			isokey = false
      			return false
      		}
      		 if($(a).attr('name') == 'email' && !(/^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/.test($(a).val()))){
        		alert('邮箱有误');
        		isokey = false;
        		return false;
        	}
      		if($(a).attr('name')=="wxid" && $(a).val() == ''){
      			alert('微信不能为空')
      			isokey = false
      			return false
      		}
  			if($(a).attr('name')=="company" && $(a).val()==''){
  				alert('公司名称不能为空')
      			isokey = false
      			return false
  			}
  			if($(a).attr('name')=="address" && $(a).val()==''){
  				alert('公司地址不能为空')
      			isokey = false
      			return false
  			}
  			if($(a).attr('name')=="companyphone" && $(a).val()==''){
  				alert('公司电话不能为空')
      			isokey = false
      			return false
  			}
       		if($(a).attr('name') == 'companyphone' && !(/^[\d\-]+$/.test($(a).val()))){
       			alert('公司电话有误');
       			isokey = false;
       			return false;
       		} 
       		if($(a).attr('name')=='values1'&& $(a).val()==''){
       			alert('第一意向不能为空')
       			isokey = false;
       			return false;
       		}
      	})
      
      	 if(isokey)submitFrom_3()
      }	
    
   	function submitFrom_3(){
        var isokey = true;
   		 $('#textList textarea').each(function(e,a){
   			if($(a).attr('name')=='values4'&& $(a).val()==''){
   	 			alert('关键资源、关键能力、竞争优势信息不能为空')
   	 			isokey  = false;
   	 			return false;
   	 		}
