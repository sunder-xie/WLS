<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.supplier.SupplierManager" table="SUPPLIER" >
		<id name="id" column="ID" type="long">
			<generator class="identity" />
		</id>
		<property name="suppname" column="SUPPNAME" type="string" />
		<property name="delstatus" column="DELSTATUS" type="string" />
	</class>
</hibernate-mapping>


