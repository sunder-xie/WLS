<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.notice.NoticeDetail" table="NOTICEDETAIL" >
		<id name="id" column="ID" type="long">
			<generator class="identity"/>
		</id>
		<property name="noticeid" column="NOTICEID" type="long"/>
		<property name="shopid" column="SHOPID" type="long" />
		<property name="begintime" column="BEGINTIME" type="timestamp"/>
		<property name="endtime" column="ENDTIME" type="timestamp"/>
		<property name="updatetime" column="UPDATETIME" type="timestamp" />
		<property name="delstatus" column="DELSTATUS" type="string" />
	</class>
</hibernate-mapping>

