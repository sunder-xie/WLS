<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.notice.NoticeManage" table="NOTICEMANAGE" >
		<id name="id" column="ID" type="long">
			<generator class="identity"/>
		</id>
		<property name="tradeno" column="TRADENO" type="string"/>
		<property name="noticename" column="NOTICENAME" type="string" />
		<property name="shopname" column="SHOPNAME" type="string" />
		<property name="shopids" column="SHOPID" type="string"/>
		<property name="status"	column="STATUS" type="string"/>
		<property name="begintime" column="BEGINTIME" type="timestamp"/>
		<property name="endtime" column="ENDTIME" type="timestamp"/>
		<property name="delstatus" column="DELSTATUS" type="string" />
		<property name="content" column="CONTENT" type="string" />
	</class>
</hibernate-mapping>

