<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.company.CompanyActivity" table="COMPANYACTIVITY">
		<id name="id" column="ID" type="long">
			<generator class="identity"/>
		</id>
		<property name="shopid" column="SHOPID" />
		<property name="companyid" column="OPERATORID" />
		<property name="companyinfo" column="OPERATORINFO" />
		<property name="companystatus" column="OPERATORSTATUS" />
		<property name="shopname" column="SHOPNAME" />
		<property name="companyname" column="COMPANYNAME" />
		<property name="telephone" column="TELEPHONE" />
		<property name="contants" column="CONTANTS" />
	</class>
</hibernate-mapping>

