<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wheelys.model.ProductShop" table="wheelys_productshop" >
		<id name="psid" column="psid" type="long">
			<generator class="identity" />
		</id>
		<property name="psshopid" column="psshopid" type="long" />
		<property name="psprodid" column="psprodid" type="long" />
		<property name="psorder" column="psorder" type="integer" />
		<property name="display" column="display" type="integer" />
		<property name="discount" column="discount" type="float" />
		<property name="delstatus" column="delstatus" type="string" />
	</class>
</hibernate-mapping>

